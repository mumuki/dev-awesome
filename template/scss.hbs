@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
  font-weight:normal;
  font-style:normal;
}

.{{../baseSelector}} {
	line-height: 1;
}

.{{../baseSelector}}:before {
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
	vertical-align: top;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "{{this}}";
}

.{{../classPrefix}}{{@key}}-c:before {
  content: "\2001";
  background-size: 100% 100%;
  background-image: url("../fonts/dev-awesome/svg/colored/{{@key}}.svg");
}
{{/each}}